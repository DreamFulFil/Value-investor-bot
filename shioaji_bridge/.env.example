# Shioaji Bridge Environment Variables
# Copy this file to .env and fill in your actual credentials
# NEVER commit the actual .env file to version control

# Shioaji API Credentials (required)
# Obtain these from Sinopac/Shioaji account management
SHIOAJI_API_KEY=your_api_key_here
SHIOAJI_SECRET_KEY=your_secret_key_here
SHIOAJI_PERSON_ID=your_person_id_here

# Optional Settings
# Set to 'true' for simulation mode (paper trading)
SHIOAJI_SIMULATION=false

# Timeout in seconds for API operations
SHIOAJI_TIMEOUT=30

# Logging level (DEBUG, INFO, WARNING, ERROR)
SHIOAJI_LOG_LEVEL=INFO
